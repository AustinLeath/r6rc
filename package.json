{
  "name": "R6RC",
  "version": "0.3.5",
  "main": "main.js",
  "license": "MIT",
  "description": "A Minimalistic Rank Calculator For Rainbow Six: Siege",
  "author": "Austin Leath",
  "scripts": {
    "start": "electron .",
    "testwin": "build --win --dir",
    "testmac": "build --mac  --dir",
    "publish": "build -p always",
    "updep": "npm install electron@latest electron-builder@latest electron-packager@latest electron-log@latest electron-updater@latest"
  },
  "devDependencies": {
    "electron": "^2.0.3",
    "electron-builder": "^20.15.1",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "electron-log": "^2.2.15",
    "electron-updater": "^2.21.10"
  },
  "build": {
    "appId": "com.austinleath.r6rc",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
